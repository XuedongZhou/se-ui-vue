{
  "customSyntax": "postcss-scss",
  "extends": ["stylelint-config-recess-order", "stylelint-config-standard-scss", "stylelint-config-prettier-scss"],
  "plugins": ["stylelint-order", "stylelint-declaration-block-no-ignored-properties"],
  "rules": {
